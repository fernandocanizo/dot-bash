export GBT_CARS='Status, Time, Hostname, Dir, Git, ExecTime, Sign'

# full pwd
export GBT_CAR_DIR_DEPTH='9999'

export GBT_CAR_TIME_TIME_FORMAT='03:04:05'

export GBT_CAR_STATUS_DISPLAY='1'

export GBT_CAR_TIME_DATE_FORMAT='2006.01.02'

# ExecTime

# show running time below car
export GBT_CAR_EXECTIME_WRAP='1'
# load it
source /usr/share/gbt/sources/exectime/bash.sh

# Prompt forwarding
export GBT__HOME='/usr/share/gbt'
source $GBT__HOME/sources/gbts/cmd/local.sh

